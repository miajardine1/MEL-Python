global proc Mean(float $num1, float $num2, float $num3, float $num4, float $num5)
{
    float $mean = ($num1 + $num2 + $num3 + $num4 + $num5)/5;
    print("The average of " + $num1 + ", " + $num2 + ", " + $num3 + ", " + $num4 + ", & " + $num5 + " is " + $mean + ".\n");
}

Mean(1,2,4,5,6);

